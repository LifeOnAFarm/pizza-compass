{
    "extensions.ignoreRecommendations": true,
    "C_Cpp.default.configurationProvider": "particle.particle-vscode-core",
    "cortex-debug.openocdPath": "${command:particle.getDebuggerOpenocdPath}",
    "files.associations": {
        "*.ino": "cpp",
        "cmath": "cpp"
    },
    "particle.targetPlatform": "boron",
    "particle.firmwareVersion": "2.0.1",
    "particle.enableVerboseLocalCompilerLogging": true,
    "particle.targetDevice": "pizza_compass",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingOvertype": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoIndent": "none",
    "editor.autoSurround": "never",
    "particle.disableLocalCompilerDirtyCheck": true,
    "typescript.tsdk": "",
    "git.enabled": false,
    "git.path": null,
    "git.autofetch": false
}